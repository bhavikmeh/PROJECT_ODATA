<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout">
	<ResponsivePopover id="myPopover" title="Sales Order {SalesOrderID}" class="sapUiContentPadding">
		<beginButton>
			<Button text="Close" press="handleCloseButton"/>
		</beginButton>
		<content>
			<l:Grid>
				<Text text="Gross Amount: {GrossAmount}"/>
				<Text text="Net Amount: {NetAmount}"/>
				<Text text="Tax Amount: {TaxAmount}"/>
			</l:Grid>
			<Image
				src="data:image/jpeg;base64,"
				width="5em"/>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>